@article{liutaoRechuliduiTC4taihejindongtailixuexingnengheweiguanzuzhideyingxiang,
	title = {{热处理对TC4钛合金动态力学性能和微观组织的影响}},
	issn = {1000-4939},
	url = {https://kns.cnki.net/kcms2/article/abstract?v=3uoqIhG8C45S0n9fL2suRadTyEVl2pW9UrhTDCdPD66eYqZwZrKZ0dnWJlGix6u5gPetNK2uEjhMKIlJiI1zziw4Ih59SrqF&uniplatform=NZKPT},
	abstract = {为揭示热处理对TC4钛合金动态力学性能及微观组织的影响，选取两种典型热处理方式和五种加载应变率开展了TC4钛合金试样的动态力学性能实验，获取了动态应力-应变数据，并进行了试样的XRD和金相分析。结果表明：高应变率下TC4钛合金应变率强化效应显著。时效处理后，TC4钛合金流动应力、屈服强度及抗压强度得到提升，而固溶时效处理后上述性能降低。时效处理和未热处理试样应力应变曲线均具有弹性、屈服和塑性阶段，而固溶时效处理后无明显弹性和屈服阶段。固溶时效处理后流动应力随应变率增加而增加，时效处理和未热处理试样流动应力无明显变化。时效处理后试样等轴初生α相显著增大且β相含量较低，固溶时效处理后α相晶界增大且含有针状α的β转变基体，TC4钛合金力学性能与β相和亚稳β相的马氏体转变有关。},
	language = {中文},
	urldate = {2023-04-02},
	journal = {应用力学学报},
	author = {{刘涛} and {柏威} and {吴乔国} and {孙虹} and {雷经发}},
	note = {{\textless}北大核心, CSCD{\textgreater}},
	keywords = {热处理, /unread, 🔥},
	pages = {1--10},
	file = {Full Text PDF:C\:\\Users\\agape\\Zotero\\storage\\QJFGK92Q\\刘涛 等 - 热处理对TC4钛合金动态力学性能和微观组织的影响.pdf:application/pdf},
}

